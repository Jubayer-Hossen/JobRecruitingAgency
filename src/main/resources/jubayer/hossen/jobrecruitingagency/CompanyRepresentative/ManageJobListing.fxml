<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jubayer.hossen.jobrecruitingagency.CompanyRepresentative.ManageJobListingController">
   <children>
      <Label layoutX="335.0" layoutY="37.0" text="Job Listings">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <TableView fx:id="jobTableView" layoutX="56.0" layoutY="156.0" prefHeight="200.0" prefWidth="687.0">
        <columns>
          <TableColumn fx:id="titleTC" prefWidth="168.00006103515625" text="Title" />
          <TableColumn fx:id="statusTC" prefWidth="178.39996337890625" text="Status" />
            <TableColumn fx:id="postedOnTC" prefWidth="166.39996337890625" text="Posted On" />
            <TableColumn fx:id="applicationsTC" prefWidth="176.00006103515625" text="Applications" />
        </columns>
      </TableView>
      <Button layoutX="69.0" layoutY="436.0" mnemonicParsing="false" onAction="#editJobOA" prefHeight="30.0" prefWidth="130.0" text="Edit">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="250.0" layoutY="435.0" mnemonicParsing="false" onAction="#duplicateJobOA" prefHeight="30.0" prefWidth="130.0" text="Duplicate">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="442.0" layoutY="432.0" mnemonicParsing="false" onAction="#closeJobOA" prefHeight="30.0" prefWidth="130.0" text="Pause / Close">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="626.0" layoutY="433.0" mnemonicParsing="false" onAction="#createNewJobOA" prefHeight="30.0" prefWidth="130.0" text="New Job">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
