<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/24" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jubayer.hossen.jobrecruitingagency.BusinessDevelopmentExecutive.ControllerClasses.ChangePasswordController">
    <children>
        <Label layoutX="315.0" layoutY="111.0" text="Enter Old Password">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label layoutX="315.0" layoutY="181.0" text="Enter New Password">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label layoutX="315.0" layoutY="250.0" text="Re-enter New Password">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <PasswordField fx:id="oldPasswordPasswordField" layoutX="315.0" layoutY="135.0" prefHeight="25.0" prefWidth="170.0" />
        <PasswordField fx:id="newPasswordPasswordField" layoutX="315.0" layoutY="205.0" prefHeight="25.0" prefWidth="170.0" />
        <PasswordField fx:id="reEnterNewPasswordPasswordField" layoutX="315.0" layoutY="275.0" prefHeight="25.0" prefWidth="170.0" />
        <Button layoutX="375.0" layoutY="330.0" mnemonicParsing="false" onAction="#changePasswordButtonOnAction" text="Change Password" />
        <Button layoutX="40.0" layoutY="30.0" mnemonicParsing="false" onAction="#backHomeButtonOnAction" text="← Back" />
        <Button layoutX="485.0" layoutY="205.0" mnemonicParsing="false" onAction="#showPasswordButtonOnAction" prefHeight="25.0" prefWidth="110.0" text="Show Passwords" />
    </children>
</AnchorPane>
